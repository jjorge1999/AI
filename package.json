    "@angular/compiler-cli": "^17.3.0",
    "@nx/angular": "21.1.3",
    "@nx/cypress": "^21.1.3",
    "@nx/playwright": "^21.1.3",
    "@nx/workspace": "21.1.3",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "^17.3.17",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "concurrently": "^9.2.1",
    "cypress": "^14.2.1",
    "eruda": "^3.4.3",
    "jasmine-core": "~5.1.0",
