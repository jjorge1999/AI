{
  "name": "jjm-inventory",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "build:web": "nx build --configuration production",
    "apk:release": "npm run build:web && npx cap copy android && cd android && gradlew.bat assembleRelease",
    "dev": "concurrently \"npm start\" \"npm run dev --prefix backend\"",
    "backend": "npm run dev --prefix backend",
    "e2e": "cypress run --config-file e2e/cypress.config.ts",
    "e2e:open": "cypress open --config-file e2e/cypress.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
